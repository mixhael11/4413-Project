  	<servlet-name>AuctionListServlet</servlet-name>
  	<servlet-class>AuctionListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AuctionListServlet</servlet-name>
  	<url-pattern>/AuctionListServlet</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>PlaceBidServlet</servlet-name>
  	<servlet-class>PlaceBidServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PlaceBidServlet</servlet-name>
  	<url-pattern>/PlaceBidServlet</url-pattern>
  </servlet-mapping>
</web-app>